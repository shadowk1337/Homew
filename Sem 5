#include <stdio.h>
#define ArSize 50

int search (char ar1[], int ar1size, char ar2[], int ar2size);

int main() {
    char s1[ArSize];
    char s2[ArSize];
    int count1 = 0, count2 = 0;
    while (1 != 0) {
        count1 = 0, count2 = 0;
        printf("> ");
        while ((s1[count1] = getchar()) != '\n')
            count1++;
        printf("> ");
        while ((s2[count2] = getchar()) != '\n')
            count2++;
        printf("%d\n", search(s1, count1, s2, count2));
    }
    return 0;
}

int search (char ar1[], int ar1size, char ar2[], int ar2size){
    int i;
    for (i = 0; i < ar1size; i++) {
        for (int j = 0; j < ar2size; j++)
            if (ar1[i] == ar2[j]) {
                return i + 1;
            }
    }
    return -1;
}
