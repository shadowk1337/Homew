Task 1
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int strend (char* s, char* t);

int main() {
    char s[50] = "123";
    char t[50] = "123";
    printf("%d", strend(s, t));
    return 0;
}

int strend (char* s, char* t){
    int i;
    for (i = strlen(s) - strlen(t); s[i] == *t; i++, t++)
        if (*t == '\0')
            return 1;
    return 0;
}

